{
  "name": "@mananshah998/zeus-api",
  "type": "module",
  "version": "1.0.4",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "directories": {
    "lib": "lib"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon --legacy-watch lib/index.js",
    "start:prod": "node lib/index.js",
    "localdev:start": "docker-compose up --build",
    "localdev:end": "docker-compose down",
    "localdev:up": "docker-compose up",
    "logs": "docker-compose logs -f",
    "db:migrate": "knex migrate:latest",
    "db:migrate:dev": "knex migrate:latest --env development",
    "db:migrate:prod": "knex migrate:latest --env production",
    "db:seed": "knex seed:run",
    "db:seed:dev": "knex seed:run --env development",
    "db:seed:prod": "knex seed:run --env production",
    "db:rollback": "knex migrate:rollback",
    "clean-docker": "docker ps -aq | xargs -r docker stop && docker ps -aq | xargs -r docker rm && docker images -q | xargs -r docker rmi && docker volume ls -q | xargs -r docker volume rm",
    "format": "npx prettier --write .",
    "build": "mkdir -p dist && cp src/*.js dist/",
    "prepublishOnly": "npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@emailjs/nodejs": "^5.0.2",
    "@hapi/basic": "^7.0.2",
    "@hapi/cookie": "^12.0.1",
    "@hapi/hapi": "^21.3.12",
    "@slack/web-api": "^7.8.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.7",
    "hapi": "^18.1.0",
    "hapi-auth-cookie": "^10.0.0",
    "joi": "^17.13.3",
    "knex": "^3.1.0",
    "mysql": "^2.18.1",
    "nodemailer": "^6.10.0",
    "pg": "^8.13.2",
    "prettier": "^3.4.2",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^15.13.0",
    "nodemon": "^3.1.9",
    "typescript-eslint": "^8.17.0"
  }
}
